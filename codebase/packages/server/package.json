{
  "name": "@pma/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "run:dev": "cross-env NODE_PORT=4000 env-cmd --no-override -f .env ts-node-dev -r tsconfig-paths/register --respawn --inspect --no-notify --files src/index.ts",
    "run:prod": "env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} ts-node --transpile-only -- src/index.ts",
    "run:debug": "env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} node -r source-map-support/register --inspect build/index.js",
    "start:prod": "node build/index.js",
    "build:prod": "tsc -p .",
    "clean": "rimraf build",
    "clean:all": "yarn clean && rimraf node_modules"
  },
  "dependencies": {
    "@dex-ddl/core": "0.1.153",
    "@energon-connectors/core": "^0.11.2",
    "@pma-common/connector-utils": "0.1.0",
    "@pma-common/logger": "0.1.0",
    "@pma-connectors/colleague-api": "0.1.0",
    "@pma-connectors/onelogin": "0.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^2.6.1",
    "pino-http": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "redux-first-router": "^2.1.5",
    "type-is": "~1.6.18",
    "yn": "^4.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.13",
    "@types/http-proxy": "^1.17.8",
    "@types/node": "^14.14.21",
    "@types/pino": "^6.3.9",
    "@types/pino-http": "^5.4.2",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.12",
    "env-cmd": "^10.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.1.3"
  }
}
