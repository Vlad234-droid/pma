{
  "name": "@pma/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "run:dev": "cross-env NODE_PORT=4000 env-cmd --no-override -f .env ts-node-dev -r tsconfig-paths/register --respawn --debug --inspect=12000 --no-notify --files src/index.ts",
    "run:standalone:dev": "cross-env INTEGRATION_MODE=standalone NODE_ENV=development env-cmd -f .env ts-node-dev -r tsconfig-paths/register --respawn --debug --inspect=12000 --no-notify --files src/index.ts",
    "run:prod": "env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} ts-node --transpile-only -- src/index.ts",
    "run:server": "ts-node --transpile-only -- src/index.ts",
    "build": "tsc --project ./",
    "clean": "rimraf build",
    "clean:all": "yarn clean && rimraf node_modules"
  },
  "dependencies": {
    "@energon/onelogin": "^1.35.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "node-fetch": "^2.6.1",
    "redux-first-router": "^2.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/http-proxy": "^1.17.5",
    "@types/node": "^14.14.21",
    "env-cmd": "^10.1.0",
    "ts-node-dev": "^1.1.8"
  }
}
