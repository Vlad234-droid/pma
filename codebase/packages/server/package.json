{
  "name": "@pma/server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "run:dev": "env-cmd -f .env nodemon",
    "run:prod": "env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} ts-node --transpile-only -- src/index.ts",
    "build": "env-cmd -f .env.${BUILD_ENV} tsc --project ./",
    "clean": "rimraf build",
    "clean:all": "yarn clean && rimraf node_modules"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/http-proxy": "^1.17.5",
    "@types/node": "^14.14.21",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
