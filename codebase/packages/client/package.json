  "scripts": {
    "run:dev": "cross-env REACT_APP_INTEGRATION_MODE=standalone PORT=9000 env-cmd --silent --no-override --fallback -f .env react-app-rewired start",
    "build:prod": "cross-env REACT_APP_INTEGRATION_MODE=standalone env-cmd --silent --no-override --fallback -f .env.${BUILD_ENV} react-app-rewired build",
    "test:coverage": "cross-env BABEL_ENV=test react-app-rewired test --coverage --collectCoverageFrom=!src/**/*.stories.tsx --watchAll",
    "test": "cross-env BABEL_ENV=test react-app-rewired test --setupFilesAfterEnv ./setupTests.js",
    "eject": "react-scripts eject",
    "clean": "rimraf build",
    "clean:all": "yarn clean && rimraf node_modules",
    "start": "webpack serve",
    "start:standalone": "webpack serve --env standalone",
    "run:standalone:dev": "webpack serve --env standalone",
    "build": "webpack --mode=development",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
