{
  "testEnvironment": "jsdom",
  "collectCoverageFrom": [
    "src/**/*.{ts,js,tsx,jsx}",
    "!src/root.component.tsx",
    "!src/**/index.{ts,js}",
    "!src/**/*.stories.*"
  ],
  "moduleDirectories": ["node_modules", "src"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/test/mocks/fileMock.ts"
  }
}
