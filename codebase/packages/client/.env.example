# Specifies the port number, which will be used for development server
PORT=9000

NODE_ENV=development

# see: https://skryvets.com/blog/2018/09/20/an-elegant-solution-of-deploying-react-app-into-a-subdirectory/
PUBLIC_URL=""

# =======================================
# React Application scope variables
# All should be prefixed with: REACT_APP_
# =======================================

REACT_APP_LOGOUT_URL=/sso/logout

REACT_APP_API_URL=/api/v1
REACT_APP_MY_INBOX_API_PATH=/api/colleague-inbox

# could be integrity or standalone
REACT_APP_INTEGRATION_MODE=standalone

